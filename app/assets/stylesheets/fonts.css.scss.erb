@charset "UTF-8";

@font-face {
  font-family: "origins";

  src:url(<%= asset_path("fonts/origins.eot") %>);
  src:url(<%= asset_path("fonts/origins.eot?#iefix") %>) format("embedded-opentype"),
    url(<%= asset_path("fonts/origins.woff") %>) format("woff"),
    url(<%= asset_path("fonts/origins.ttf") %>) format("truetype"),
    url(<%= asset_path("fonts/origins.svg#origins") %>) format("svg");
  font-weight: normal;
  font-style: normal;

}


[data-icon]:before {
  font-family: "origins" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="icon-"]:before,
[class*=" icon-"]:before {
  font-family: "origins" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-facebook:before {
  content: "a";
}
.icon-pinterest:before {
  content: "b";
}
.icon-instagram:before {
  content: "d";
}
.icon-twitter-1:before {
  content: "e";
}
.icon-youtube-play:before {
  content: "f";
}
.icon-drop:before {
  content: "c";
}
.icon-nav:before {
  content: "g";
}
.icon-arrow-right:before {
  content: "h";
}
.icon-nav-close:before {
  content: "i";
}
.icon-crosshairpinpoint:before {
  content: "j";
}


